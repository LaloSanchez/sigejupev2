class Tarifas {

    private $cveTarifa;
    private $cveTipoTarifa;
    private $costo;
    private $detalle;
    private $inicioVigencia;
    private $finVigencia;
    private $activo;
    private $fechaActualizacion;
    private $fechaRegistro;
    
    function __construct() {
        
    }

    function getCvetarifa() {
        return $this->cveTarifa;
    }

    function getCveTipoTarifa() {
        return $this->cveTipoTarifa;
    }

    function getCosto() {
        return $this->costo;
    }

    function getDetalle() {
        return $this->detalle;
    }

    function getInicioVigencia() {
        return $this->inicioVigencia;
    }

    function getFinVigencia() {
        return $this->finVigencia;
    }

    function getActivo() {
        return $this->activo;
    }

    function getFechaActualizacion() {
        return $this->fechaActualizacion;
    }

    function getFechaRegistro() {
        return $this->fechaRegistro;
    }

    function setCvetarifa($cvetarifa) {
        $this->cveTarifa = $cvetarifa;
        return $this;
    }

    function setCveTipoTarifa($cveTipoTarifa) {
        $this->cveTipoTarifa = $cveTipoTarifa;
        return $this;
    }

    function setCosto($costo) {
        $this->costo = $costo;
        return $this;
    }

    function setDetalle($detalle) {
        $this->detalle = $detalle;
        return $this;
    }

    function setInicioVigencia($inicioVigencia) {
        $this->inicioVigencia = $inicioVigencia;
        return $this;
    }

    function setFinVigencia($finVigencia) {
        $this->finVigencia = $finVigencia;
        return $this;
    }

    function setActivo($activo) {
        $this->activo = $activo;
        return $this;
    }

    function setFechaActualizacion($fechaActualizacion) {
        $this->fechaActualizacion = $fechaActualizacion;
        return $this;
    }

    function setFechaRegistro($fechaRegistro) {
        $this->fechaRegistro = $fechaRegistro;
        return $this;
    }



    
}

?>