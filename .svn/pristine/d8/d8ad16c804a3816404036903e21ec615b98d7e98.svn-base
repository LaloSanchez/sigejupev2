class ImputadossentenciasDTO {
    private $idImputadoSentencia;
    private $idActuacion;
    private $idImpOfeDelCarpeta;
    private $Apelacion;
    private $activo;
    private $fechaRegistro;
    private $fechaActualizacion;
    public function getIdImputadoSentencia(){
        return $this->idImputadoSentencia;
    }
    public function setIdImputadoSentencia($idImputadoSentencia){
        $this->idImputadoSentencia=$idImputadoSentencia;
    }
    public function getIdActuacion(){
        return $this->idActuacion;
    }
    public function setIdActuacion($idActuacion){
        $this->idActuacion=$idActuacion;
    }
    public function getIdImpOfeDelCarpeta(){
        return $this->idImpOfeDelCarpeta;
    }
    public function setIdImpOfeDelCarpeta($idImpOfeDelCarpeta){
        $this->idImpOfeDelCarpeta=$idImpOfeDelCarpeta;
    }
    public function getApelacion(){
        return $this->Apelacion;
    }
    public function setApelacion($Apelacion){
        $this->Apelacion=$Apelacion;
    }
    public function getActivo(){
        return $this->activo;
    }
    public function setActivo($activo){
        $this->activo=$activo;
    }
    public function getFechaRegistro(){
        return $this->fechaRegistro;
    }
    public function setFechaRegistro($fechaRegistro){
        $this->fechaRegistro=$fechaRegistro;
    }
    public function getFechaActualizacion(){
        return $this->fechaActualizacion;
    }
    public function setFechaActualizacion($fechaActualizacion){
        $this->fechaActualizacion=$fechaActualizacion;
    }
    public function toString(){
        return array("idImputadoSentencia"=>$this->idImputadoSentencia,
"idActuacion"=>$this->idActuacion,
"idImpOfeDelCarpeta"=>$this->idImpOfeDelCarpeta,
"Apelacion"=>$this->Apelacion,
"activo"=>$this->activo,
"fechaRegistro"=>$this->fechaRegistro,
"fechaActualizacion"=>$this->fechaActualizacion);
    }
}
?>