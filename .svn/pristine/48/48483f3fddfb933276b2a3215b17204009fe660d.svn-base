                        //tabla += "<td style='display:none;' class='td_datos' id='td_"+contador+"' idSecuencia='"+val.iId_Secuencia+"' idUltimoRol='"+val.iId_UltimoRol+"' iId_Programacion='"+val.iId_Programacion+"' iId_RolJuzgador='"+val.iId_RolJuzgador+"' iAparicion='"+val.iAparicion+"' iNumSemana='"+val.iNumSemana+"'>";
                        //tabla += "</td>";
                    tabla += "</tr>";
                    //contador++;
                });
            } catch (e) {
                alert("Error al cargar Juzgadores:" + e);
            }
