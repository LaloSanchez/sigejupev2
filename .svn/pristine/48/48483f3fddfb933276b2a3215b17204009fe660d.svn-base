        error: function (objeto, quepaso, otroobj) {
            alert("Error en la peticion de juzgadores:\n\n" + otroobj);
        }
    });
    $("#tablaJuzgadores tbody").append(tabla);
$("#paginadorJuzgadores").empty();
var paginacion = "";
