class TipopersonaasuntoDTO {
    private $cveTipoPersonaAsunto;
    private $Descripcion;
    private $Activo;
    public function getCveTipoPersonaAsunto(){
        return $this->cveTipoPersonaAsunto;
    }
    public function setCveTipoPersonaAsunto($cveTipoPersonaAsunto){
        $this->cveTipoPersonaAsunto=$cveTipoPersonaAsunto;
    }
    public function getDescripcion(){
        return $this->Descripcion;
    }
    public function setDescripcion($Descripcion){
        $this->Descripcion=$Descripcion;
    }
    public function getActivo(){
        return $this->Activo;
    }
    public function setActivo($Activo){
        $this->Activo=$Activo;
    }
    public function toString(){
        return array("cveTipoPersonaAsunto"=>$this->cveTipoPersonaAsunto,
"Descripcion"=>$this->Descripcion,
"Activo"=>$this->Activo);
    }
}
?>