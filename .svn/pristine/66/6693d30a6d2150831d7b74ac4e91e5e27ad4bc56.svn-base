class AsignarecursosDTO {
    private $idAsignaRecurso;
    private $idReferencia;
    private $numDiscos;
    private $idJuzgador;
    private $activo;
    private $fechaRegistro;
    private $fechaActualizacion;
    public function getIdAsignaRecurso(){
        return $this->idAsignaRecurso;
    }
    public function setIdAsignaRecurso($idAsignaRecurso){
        $this->idAsignaRecurso=$idAsignaRecurso;
    }
    public function getIdReferencia(){
        return $this->idReferencia;
    }
    public function setIdReferencia($idReferencia){
        $this->idReferencia=$idReferencia;
    }
    public function getNumDiscos(){
        return $this->numDiscos;
    }
    public function setNumDiscos($numDiscos){
        $this->numDiscos=$numDiscos;
    }
    public function getIdJuzgador(){
        return $this->idJuzgador;
    }
    public function setIdJuzgador($idJuzgador){
        $this->idJuzgador=$idJuzgador;
    }
    public function getActivo(){
        return $this->activo;
    }
    public function setActivo($activo){
        $this->activo=$activo;
    }
    public function getFechaRegistro(){
        return $this->fechaRegistro;
    }
    public function setFechaRegistro($fechaRegistro){
        $this->fechaRegistro=$fechaRegistro;
    }
    public function getFechaActualizacion(){
        return $this->fechaActualizacion;
    }
    public function setFechaActualizacion($fechaActualizacion){
        $this->fechaActualizacion=$fechaActualizacion;
    }
    public function toString(){
        return array("idAsignaRecurso"=>$this->idAsignaRecurso,
"idReferencia"=>$this->idReferencia,
"numDiscos"=>$this->numDiscos,
"idJuzgador"=>$this->idJuzgador,
"activo"=>$this->activo,
"fechaRegistro"=>$this->fechaRegistro,
"fechaActualizacion"=>$this->fechaActualizacion);
    }
}
?>