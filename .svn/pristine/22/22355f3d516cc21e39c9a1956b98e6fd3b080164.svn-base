                $cp = ($domicilio->getCp() == '') ? '' : 'C.P: ' . $domicilio->getCp() . '.';

                $domicilio_completo = ($direccion == '' && $colonia == '' && $cp == '') ? 'N/A' : $direccion . $colonia . $cp;

                $array_domicilios[] = utf8_encode($domicilio_completo);
            }
        }


        return $array_domicilios;
