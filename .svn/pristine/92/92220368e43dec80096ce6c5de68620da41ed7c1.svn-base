div#divFormCambiaPass input.password { margin-bottom:0px; width:95%; padding: .4em; }
div#divFormCambiaPass { width: 400px; margin: 0px; padding: 0px; }
div#divFormCambiaPass table { margin: 1em 0; border-collapse: collapse; width: 100%; }
div#divFormCambiaPass table td, div#divFormCambiaPass table th { border: 1px solid #eee; padding: .6em 10px; text-align: left; }
div#divFormCambiaPass label, input { display:block; }
div#divFormCambiaPass .validateTips { border: 1px solid transparent; padding: 0 0.3em 0.3em 0.3em; }
