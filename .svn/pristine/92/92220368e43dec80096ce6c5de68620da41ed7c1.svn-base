    float: left;
    vertical-align: middle;
    padding: 0px 5px;
    height: 36px;
    /*border: 1px solid #FF0000*/
}

ul#ulMenuSup li img.imgBtnMenu{
    cursor: pointer;
    border: 0px;
    margin-top: 2px;
}

ul#ulMenuSup li.opcMenuSup{
    display: none;
}

