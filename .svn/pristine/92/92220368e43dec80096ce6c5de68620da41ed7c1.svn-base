#divOpac {
	z-index: 0;
	position: absolute;
	top: 0px;
	left: 0px;
	width: 100%;
	height: 100%;
	background-color: #CCCCCC;
	filter: alpha(opacity=80);
	opacity: 0.8;
}
div#divOpac iframe{
	position:absolute;
	top: 0px;
	left: 0px;
	width: 100%;
	height: 100%;
	background-color: #CCCCCC;
	filter: alpha(opacity=30);
	opacity: 0.3;
}
